#include <stdio.h>
int main() {
float a,b,c,d,m;
printf("Enter co-ordanites of point A\n");
printf ("x-axis:");
scanf("%f",&a);
printf("y-axis:");
scanf("%f",&b);
printf("\n");
printf("Enter co-ordinates of point B\n");
printf ("x-axis:");
scanf("%f",&c);

printf("y-axis:");
scanf("%f",&d);
m= (d-b)/(c-a);
printf("Slope of AB: %.3f",m);
    return 0;
}
