#include <stdio.h>
int main() {
float a,b,c,d,m;
printf("Enter co-ordanites of point A\n");
printf ("x-axis:");
scanf("%f",&a);
printf("\n");
printf("y-axis:");
scanf("%f",&b);
printf("Enter co-ordinates of point B\n");
printf ("x-axis:");
scanf("%f",&c);
printf("\n");
printf("y-axis:");
scanf("%f",&d);
m= (d-b)/(c-a);
printf("%.3f",m);
    return 0;
}
