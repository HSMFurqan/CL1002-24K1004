#include <stdio.h>
int main() {
printf("Number Reverser\n");
int a,b,d1,d2,d3,d4,d5;
printf ("Enter a 5 digit number:");
scanf("%i",&a);
d1= a%10;
a=a/10;
d1=d1*10000;
d2=a%10;
a=a/10;
d2=d2*1000;
d3=a%10;
d3=d3*100;
a=a/10;
d4=a%10;
d4=d4*10;
a=a/10;
d5=a%10;
a=a/10;
b=d1+d2+d3+d4+d5;
printf("Result=%i",b);
    return 0;
}

#Switch Calculator:
#include <stdio.h>
int main() {
float a,b,c;
char op;
printf("Enter number:");
scanf("%f",&a);
printf("Enter number:");
scanf("%f",&b);
getchar();
printf("Enter operator:");
scanf("%c",&op);
switch(op){
case '+':
		c=a+b;
break;
case '-':
		c=a-b;
break;
case '*':
		c=a*b;
break;
case '/':
		if(b==0){
			printf("Mathematical error");
		}
	    else{
	         c=a/b;  }
break;
    	}
printf("\nResult=%.2f",c);
    return 0;
}
