#include <stdio.h>
int main() {
printf("Number Reverser\n");
int a,b,d1,d2,d3,d4,d5;
printf ("Enter a 5 digit number:");
scanf("%i",&a);
d1= a%10;
a=a/10;
d1=d1*10000;
d2=a%10;
a=a/10;
d2=d2*1000;
d3=a%10;
d3=d3*100;
a=a/10;
d4=a%10;
d4=d4*10;
a=a/10;
d5=a%10;
a=a/10;
b=d1+d2+d3+d4+d5;
printf("Result=%i",b);
    return 0;
}
